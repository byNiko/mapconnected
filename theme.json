{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"useRootPaddingAwareAlignments": true,
		"appearanceTools": true,
		"color": {
			"background": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultGradients": true,
			"duotone": [],
			"gradients": [
				{
					"name": "Primary Gradient",
					"slug": "primary-gradient",
					"gradient": "linear-gradient(0deg, var(--wp--preset--color--primary) 0%, var(--wp--preset--color--primary-text) 100%)"
				}
			],
			"link": false,
			"text": true,
			"defaultPalette": false,
			"palette": [
				{
					"name": "Real Black",
					"slug": "black-real",
					"color": "#000000"
				},
				{
					"name": "Real White",
					"slug": "white-real",
					"color": "#fff"
				},
				{
					"name": "Primary",
					"slug": "primary",
					"color": "#017969"
				},
				{
					"name": "Primary 33",
					"slug": "primary-33",
					"color": "#F3F7F3"
				},
				{
					"name": "Primary 50",
					"slug": "primary-50",
					"color": "#EBF2EC"
				},
				{
					"name": "Primary 75",
					"slug": "primary-75",
					"color": "#B2CFCB"
				},
				{
					"name": "Primary Text",
					"slug": "primary-text",
					"color": "#055f52"
				},
				{
					"name": "Accent",
					"slug": "accent",
					"color": "#ffc223"
				}
			]
		},
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"dimensions": {
			"aspectRatio": false,
			"minHeight": false
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1280px"
		},
		"spacing": {
			"blockGap": null,
			"margin": true,
			"padding": true,
			"customSpacingSize": true,
			"units": ["px", "em", "rem", "vh", "vw", "%"],
			"spacingScale": {
				"operator": "*",
				"increment": 1.5,
				"steps": 7,
				"mediumStep": 1.5,
				"unit": "rem"
			},
			"spacingSizes": []
		},
		"typography": {
			"customFontSize": true,
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "var(--font-family--display)",
					"name": "Headings & Display (Ubuntu)",
					"slug": "ubuntuSans"
				}
			],
			"fontSizes": [
				{
					"name": "X-Small",
					"size": ".8rem",
					"slug": "xs"
				},
				{
					"name": "Small",
					"size": ".9rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "1rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "1.2rem",
					"slug": "large"
				},
				{
					"name": "Xl",
					"size": "1.5rem",
					"slug": "xl"
				},
				{
					"name": "2Xl",
					"size": "2rem",
					"slug": "2xl"
				},
				{
					"name": "3Xl",
					"size": "2.5rem",
					"slug": "3xl"
				},
				{
					"name": "4Xl",
					"size": "4rem",
					"slug": "4xl"
				}
			],
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"lineHeight": false,
			"textAlign": true,
			"textColumns": false,
			"textDecoration": true,
			"textTransform": true
		},
		"blocks": {
			"core/button": {
				"typography": {
				},
				"color": {
					"text": true
				}
			},
			"core/heading": {
				"typography": {}
			}
		},
		"custom": {
			"line-height": {
				"body": 1.7,
				"heading": 1.3
			}
		}
	},
	"styles": {
		"blocks": {
			"core/heading": {
				"variations": {
					"display": {
						"background": {
						},
						"typography": {
							"fontSize": "var(--font-size--xxxxl)", 
							"letterSpacing": "-.07em",
							"fontWeight": "800",
							"lineHeight": "1.2"
						}
					}
				}
			},
			"core/button": {
				"variations": {
					"outline": {

					},
					"accent":{
						"color": {
							"background": "var(--wp--preset--color--accent)"
						},
						"border": {
							"style": "solid",
							"width": "2px",
							"color": "var(--wp--preset--color--accent)"
						}
					},
					"accent-outline":{
						"color": {
							"background": "transparent",
							"text": "var(--wp--preset--color--accent)"
						},
						"border": {
							"style": "solid",
							"width": "2px",
							"color": "currentColor"
						}
					}

				},
				"spacing": {
					"padding": {
						"top": "var(--spacer-3) ",
						"right": "var(--spacer-5)",
						"bottom": "var(--spacer-3)",
						"left": "var(--spacer-5)"
					}
				},
				"color": {
					"background": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"fontWeight": "500",
					"fontSize": "var(--font-size--md)",
					"lineHeight": "1"
				},
				"border": {
					"radius": "var(--radius-btn)"
				}
			}
		},
		"spacing": {
			"margin": {
				"top": "0px",
				"right": "0px",
				"bottom": "0px",
				"left": "0px"
			},
			"padding": {
				"top": "0",
				"right": "40px",
				"bottom": "0px",
				"left": "40px"
			}
		},
		"color": {
			"background": "var(--body-bg)"
		},
		"typography": {
			"fontSize": "var(--font-size--md)",
			"fontWeight": "300"
		},
		"elements": {
			"h1": {},
			"heading": {
				"typography": {
					"fontFamily": "var(--font-family--display)"
				}
			},
			"link": {
				"color": {
					"text": "var(--color-link)"
				},
				":hover": {
				},
				":focus": {
					"typography": {
						"textDecoration": "none"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--secondary)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				"typography": {
					"textDecoration": "none"
				}
			},
			"h2": {}
		}
	}
}
